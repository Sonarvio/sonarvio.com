typographic()

html {
  box-sizing: border-box;
}

*, *:before, *:after {
  box-sizing: inherit;
  border: 0;
  margin: 0;
  padding: 0;
}

html, body {
  width:  100%;
  height: 100%;
}

.root, .App {
  height: 100%;
  background: #333;
}

.App {
  overflow-x: hidden;

  &__Info {
    margin: 50px 0;
  }
}
